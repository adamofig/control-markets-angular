{
  "name": "control-markets-angular",
  "config": {
    "appId": "markets.control.app",
    "appIdDev": "markets.control.app.dev"
  },
  "version": "0.0.45",
  "author": "Adamo",
  "homepage": "https://github.com/adamofig/control-markets-angular",
  "scripts": {
    "ng": "ng",
    "config:internalnet": "cp public/config.internalnet.json public/config.json",
    "config:homelab": "cp public/config.homelab.json public/config.json",
    "config:homeserver": "cp public/config.homeserver.json public/config.json",
    "start": "ng serve --host 0.0.0.0 --port 8120",
    "start:qa": "ng serve -c qa --host 0.0.0.0 --port 8120",
    "start:prod": "ng serve -c production --host 0.0.0.0 --port 8120",
    "prebuild": "pnpm version patch --no-git-tag-version && pnpm run sync-version",
    "build": "ng build",
    "build:dev": "ng build",
    "build:internalnet": "pnpm run config:internalnet && ng build",
    "build:homelab": "pnpm run config:homelab && ng build",
    "build:homeserver": "pnpm run config:homeserver && ng build",
    "build:prod": "ng build -c production",
    "build:internal-server": "ng build -c internal-server",
    "sync-version": "node scripts/sync-version.js",
    "deploy-prod-local": "ng build -c production & npx serve -s www/browser",
    "watch": "ng build --watch --configuration development",
    "update:dc": "pnpm install @dataclouder/app-auth@latest @dataclouder/ngx-agent-cards@latest @dataclouder/ngx-lessons@latest @dataclouder/ngx-cloud-storage@latest @dataclouder/ngx-core@latest @dataclouder/ngx-mic@latest @dataclouder/ngx-tts@latest  @dataclouder/ngx-ai-services@latest @dataclouder/ngx-users@latest @dataclouder/ngx-logs@latest @dataclouder/ngx-knowledge@latest",
    "deploy": "pnpm run build && firebase deploy --only hosting:control-markets-dev",
    "ios:dev": "ng build -c development && cap copy ios && npx cap run ios",
    "ios:prod": "ng build -c production && cap copy ios && npx cap run ios --configuration Release",
    "ios:dev-sim": "ng build -c development && cap copy ios && npx cap run ios --target 3FFDE946-E48B-4215-A7CD-D7C1872FDAF2",
    "android:debug": "ionic cap run android -l --external",
    "android:run": "npx cap run android",
    "android:open": "npx cap open android",
    "android:install-dev": "ng build -c development && cap copy android && cd android && ./gradlew assembleDev && adb install app/build/outputs/apk/dev/app-dev.apk && adb shell monkey -p markets.control.app.dev -c android.intent.category.LAUNCHER 1",
    "android:install-pro": "ng build -c production && cap copy android && cd android && ./gradlew assembleRelease && adb install app/build/outputs/apk/release/app-release.apk && adb shell monkey -p ${npm_package_config_appId} -c android.intent.category.LAUNCHER 1",
    "test": "ng test",
    "lint": "ng lint",
    "storybook": "ng run app:storybook",
    "build-storybook": "ng run app:build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~20.1.6",
    "@angular/cdk": "~20.1.5",
    "@angular/common": "~20.1.6",
    "@angular/compiler": "~20.1.6",
    "@angular/core": "~20.1.6",
    "@angular/fire": "^19.0.0",
    "@angular/forms": "~20.1.6",
    "@angular/platform-browser": "~20.1.6",
    "@angular/platform-browser-dynamic": "~20.1.6",
    "@angular/router": "~20.1.6",
    "@angular/service-worker": "~20.1.6",
    "@capacitor-community/apple-sign-in": "7.1.0",
    "@capacitor-community/speech-recognition": "7.0.1",
    "@capacitor/android": "7.4.4",
    "@capacitor/app": "7.1.0",
    "@capacitor/core": "7.4.4",
    "@capacitor/haptics": "7.0.2",
    "@capacitor/ios": "7.4.4",
    "@capacitor/keyboard": "7.0.3",
    "@capacitor/status-bar": "7.0.3",
    "@ckeditor/ckeditor5-angular": "^6.0.1",
    "@ckeditor/ckeditor5-build-balloon-block": "^38.1.0",
    "@codetrix-studio/capacitor-google-auth": "^3.4.0-rc.4",
    "@dataclouder/app-auth": "^0.0.44",
