    "@storybook/addon-docs": "^10.0.4",
    "@storybook/addon-onboarding": "^10.0.4",
    "@storybook/angular": "^10.0.4",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
