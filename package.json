    "firebase": "^12.1.0",
    "ionicons": "^8.0.13",
    "marked": "^15.0.8",
    "mermaid": "^11.4.1",
    "nanoid": "^5.1.5",
    "ngx-image-cropper": "^9.1.5",
    "ngx-markdown": "^20.1.0",
    "ngx-vflow": "^1.16.2",
    "npm-check-updates": "^17.1.16",
    "plyr": "^3.8.3",
    "postcss": "^8.5.3",
    "primeicons": "^7.0.0",
    "primeng": "^20.2.0",
    "rxjs": "~7.8.2",
    "shepherd.js": "^14.5.1",
    "swiper": "^11.2.10",
    "tailwindcss": "^4.1.4",
    "tslib": "^2.8.1",
    "turndown": "^7.2.0",
    "vis-data": "^8.0.3",
    "vis-network": "^10.0.1",
    "vis-timeline": "^8.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~20.1.6",
    "@angular-eslint/builder": "^19.3.0",
    "@angular-eslint/eslint-plugin": "^19.3.0",
